{
  "name": "youtube-downloader-api",
  "version": "1.0.0",
  "description": "A simple API to get YouTube download links using yt-dlp.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "curl -L https://github.com/yt-dlp/yt-dlp/releases/latest/download/yt-dlp -o yt-dlp && chmod a+rx ./yt-dlp"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}

